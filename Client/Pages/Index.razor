@page "/"

<h1>Transformers</h1>

<button @onclick="GoToInput">Input Data</button>

<button @onclick="GoToUpload">Upload Data</button>

@code {
    [Inject] private NavigationManager NavMan {get; set;}

    private void GoToInput()
    {
        NavMan.NavigateTo("/input");
    }

    private void GoToUpload()
    {
        NavMan.NavigateTo("/upload");
    }
}




